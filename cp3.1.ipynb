    "def multiply_list_items(lst):\n",
    "    result=1\n",
    "    for item in lst:\n",
    "        result *= item\n",
    "    return result\n",
    "\n",
    "sample_list = [2, 3, 6]\n",
    "result = multiply_list_items(sample_list)\n",
    "print(\"Result =\", result)\n"
   ]
